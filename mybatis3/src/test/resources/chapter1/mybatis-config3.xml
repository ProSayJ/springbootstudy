<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="cacheEnabled" value="true"/>
    </settings>

    <typeAliases>
        <typeAlias alias="Article" type="com.prosayj.springboot.mybatis3.execise.chapter1.model2.Article"/>
        <typeAlias alias="Author" type="com.prosayj.springboot.mybatis3.execise.chapter1.model2.Author"/>
    </typeAliases>

    <typeHandlers>
        <typeHandler javaType="com.prosayj.springboot.mybatis3.execise.constant.ArticleTypeEnum"
                     handler="com.prosayj.springboot.mybatis3.execise.ArticleTypeHandler"/>
    </typeHandlers>
</configuration>